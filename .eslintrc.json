{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
      }
    ],
    "no-plusplus": "off",
    "func-names": "off",
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
  }
}